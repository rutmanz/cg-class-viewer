{
  "name": "Veracross Class Directories",
  "manifest_version": 3,
  "description": "An extension to show mutual classes for Catlin Gabel through veracross scraping",
  "version": "0.1.2.86",
  "permissions": [
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "https://portals.veracross.com/*",
    "https://classes.veracross.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}