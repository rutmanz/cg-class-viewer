{
	"name": "Veracross Class Directories",
	"manifest_version": 2,
	"description": "An extension to show mutual classes for Veracross schools through web scraping",
	"version": "0.2.0.6",
	"permissions": [
	  "cookies",
	  "storage",
	  "https://portals.veracross.com/*",
	  "https://classes.veracross.com/*"
	],
	"browser_action": {},
	"background": {
	  "scripts": ["service-worker.js"]
	},
	"icons": {
	  "16": "icons/icon16.png",
	  "48": "icons/icon48.png",
	  "128": "icons/icon128.png"
	},
	"browser_specific_settings": {
		"gecko": {
		  "id": "veracrossext@rutmanz.github.com"
		}
	  }
	  
  }