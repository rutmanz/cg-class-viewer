{
	"name": "Veracross Class Directories",
	"manifest_version": 3,
	"description": "An extension to show mutual classes for Veracross schools through web scraping",
	"version": "0.1.0",
	"permissions": [
		"cookies",
		"storage"
	],
	"host_permissions": [
		"https://portals.veracross.com/*",
		"https://classes.veracross.com/*"
	],
  "action": {},
	"background": {
		"service_worker": "service-worker.js"
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
